{
    "name": "lukaszaleckas/laravel-circuit-breaker",
    "autoload": {
        "psr-4": {
            "LaravelCircuitBreaker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelCircuitBreaker\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.0"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "authors": [
        {
            "name": "lukaszaleckas",
            "email": "luk.zaleckas@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "laravel/framework": "^8.83|^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpstan/phpstan-mockery": "^1.0",
        "phpunit/phpunit": "^9.5",
        "slevomat/coding-standard": "^6.4",
        "squizlabs/php_codesniffer": "^3.6",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.24"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelCircuitBreaker\\CircuitBreakerServiceProvider"
            ]
        }
    }
}
